#IphotoTop a,#IphotoBottom a{
 	width:53px;
	height:53px;
	display:block;
	-moz-opacity:0.6;
	opacity:0.6;
 	filter:alpha(opacity=60);
 }
 
 #IphotoTop a:hover,#IphotoBottom a:hover{
 	filter:alpha(opacity=100);  
    -moz-opacity:1;  
    opacity:1;  
 }


.IpublicImg{
		width:170px;
