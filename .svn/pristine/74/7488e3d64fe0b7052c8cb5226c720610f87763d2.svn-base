	right:0px;
	top:2px;	
}

.Nsearch-input{
	width:200px;
	height:22px;
	margin-top:3px;
	float:left;
}

.Nsearch-btn{
	width:30px;
	height:25px;
	margin-top:3px;
	float:right;
}

#Nmain-wrapper{
	width:960px;
	height:900px; 
}

#NmainLeft-wrapper{
	width:280px;
	height:900px;
	margin-right:20px;
	float:left;

}

#Nnotice{
	width:280px;
	height:280px; 
	border-bottom:1px solid black;
}

.Nmore {
	font-size:13px;
	float:right;
}

.Nmore a{
	color:#FF0000;	
}

.Nmore a:hover{
	color:#000000; 
	text-decoration:underline;
}

.Nspan-label{
	font-size:14px;
	color:#000000;
}

 .Nnotice-view{
 	width:275px;
	height:30px;
	line-height:25px;
	padding-top:0px;
	margin-top:5px;
 }
 

 .Nnotice-views{
 	display:block; 
	color:#999999; 
	font-size:12px;
	margin-top:0px;
	float:right;
 }
 
 .NnoticeViews-title{
    width:200px; 
 	font-size:12px;
	height:22px;
	float:left;
    display:block;
	margin-top:0px;
 }
 
