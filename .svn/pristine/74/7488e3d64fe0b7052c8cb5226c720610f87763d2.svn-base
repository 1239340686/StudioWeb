	width:640px;
	height:320px;
	margin-top:250px;
	margin-left:10px;
	margin-right:10px;
}

#Ncampus-label{
	width:222px;
	height:30px;
	background-image:url(../image/Ncampus-label.png); background-repeat:no-repeat;
}

#Ncampus-label p{
	padding-left:25px;
	padding-top:6px;
	font-size:15px;
}

#Ncampus-detail{
	width:550px;
	height:270px;
	margin-top:20px;
	margin-left:45px;
	background-color:#CCFFCC;
}

 .Ncampus-view{
 	width:550px;
	height:30px;
	line-height:25px;
	padding-top:0px;
	margin-top:5px;
 }

 .Ncampus-views{
 	display:block; 
	color:#999999; 
	font-size:12px;
	margin-top:0px;
	float:right;
 }
 
 .NcampusViews-title{
    width:400px; 
 	font-size:12px;
