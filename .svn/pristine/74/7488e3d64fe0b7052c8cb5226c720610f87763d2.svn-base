/* CSS Document */
body{
	width:962px;
	height:1500px;
	margin:0 auto;
	padding:0 auto;
    font-family:"Microsoft YaHei UI";
	border:1px solid black;
}
 
 a{ 
 	text-decoration:none;
 }
 
 ul{
    list-style:none;
	margin:0px;
 }
 
 li{
    list-style:none;
 }
 
#Nheader{
	width:962px;
	height:262px;
    background-image:url(../image/index.jpg);
	position:relative;
	margin-bottom:20px;
	border:1px solid grey;
	
}

.Nheader-nav{
	width:595px;
	height:35px; 
	background-color:#e2eceb; 
	position:absolute;
	bottom:30px;
	right:50px;
}

.Nheader-nav ul{
	margin-left:0px;
	padding-left:0px;
}

.Nheader-nav ul li{
 	float:left;
	width:85px;
	font-size:14px;
	list-style:none;
	line-height:35px;
 }
 
.Nheader-nav ul li a{
 	width:85px;
	height:35px;
	display:block;
 	color:#999999;
	text-align:center;
	line-height:35px;		
 }
 
.Nheader-nav ul li a:hover{
 	background-color:#33CC66;
 }

#Ncontainer{
	width:962px;
	height:970px;
	border:1px solid #FF0000;
	
}

#Nposi-wrapper{
	width:962px;
	height:40px;
	border:1px solid #000000;
	position:relative;
	margin-bottom:10px;
}

.Nposition{
	width:286px;
	height:36px;
	font-size:14px; 
	line-height:36px;
	padding-left:1px;
	background-color:#FF3300;
	background-image:url(../image/Nposi-wapper.jpg);
	margin-top:2px;
	float:left:
}

.Nsearch{
	width:240px;
	height:36px;
