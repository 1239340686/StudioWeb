 #Nmultiple{
 	width:280px;
	height:350px;
 }
 
.Nmultiple-label{
	width:240px;
	height:30px;
	line-height:30px;
	padding-left:2px;
	background-image:url(../image/N×ÛºÏlabel_bj.gif);
}

.Nmultiple-detail{
	width:240px;
	height:170px;
}
 

#NmainRight-wrapper{
	width:660px;
	height:1100px;
	position:relative;
	float:right;
}

#Nscroll-container{
	width:640px;
	height:320px;
	top:10px;
	left:10px;
	right:10px;
	bottom:10px;
	clear:both;
	overflow:hidden;
	position:relative;
}

#Nscroll-list{
	width:4480px;
	height:320px;
	position:absolute;
	z-index:1;
	left:-640px;
}

#Nscroll-list img{
	float:left;
	width:640px;
	height:320px;
}

#Nscroll-buttons{
	position:absolute;
	height:10px;
	width:100px;
	z-index:2; 
	right:50px;bottom:20px;
}

#Nscroll-buttons span{
	width:10px;
	height:10px;
	line-height:10px;
	cursor:pointer;
	display:block;
	margin-right:2px;
	text-align:center;
	font-size:12px;
	color:#FFFFFF;
	background-color:#666666;
	float:left; 
	border:1px solid #fff;
}

#prev{
	cursor:pointer;
	/*display:none;*/
	line-height:39px;
	text-align:center;
	position:absolute;
	top:140px;
	z-index:2;
	left:20px;
}

#next{
	cursor:pointer;
	/*display:none;*/
	line-height:39px;
	text-align:center;
	position:absolute;
	z-index:2;
	top:140px;
	right:20px;
	
}
 
#NclassNews{
	width:640px;
	height:190px;
	margin-top:30px;
	background-color:#D3D3D3;
	position:absolute;
	left:10px;
	right:10px;	
}  

#Nclass-list{
	width:146px;
	height:190px;
	float:left;
	margin-right:15px;
	font-size:12px;
	color:#000000;
}

#Nclass-list img{
	border:4px solid #999999;
	width:146px;
	height:130px;
}

