<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>UserInfoCheckServlet</servlet-name>
    <servlet-class>com.bjwz.controller.UserInfoCheckServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>UserInfoDeleteServlet</servlet-name>
    <servlet-class>com.bjwz.controller.UserInfoDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>UserInfoUpdateServlet</servlet-name>
    <servlet-class>com.bjwz.controller.UserInfoUpdateServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>UserInfoServlet</servlet-name>
    <url-pattern>/servlet/UserInfoCheckServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserInfoDeleteServlet</servlet-name>
    <url-pattern>/servlet/UserInfoDeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserInfoUpdateServlet</servlet-name>
    <url-pattern>/servlet/UserInfoUpdateServlet</url-pattern>
  </servlet-mapping>

</web-app>